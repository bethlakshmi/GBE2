<div class = "schedule_section">
  <h3>
    {% if profile.display_name %}
      {{ profile.display_name }}'s
    {% else %}
       Your
     {% endif %} 
       Schedule
  </h3>	
    {% for booking in profile.schedule %}
      <div class='event'>
        {{booking.starttime}} 
        <a href="{% url 'scheduler:detail_view' booking.eventitem.eventitem_id%}">
          {{ booking }}
	</a> 
      </div> <br>
    {% endfor %}
 
</div>

