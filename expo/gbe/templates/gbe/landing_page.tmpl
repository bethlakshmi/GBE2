{% extends "base.tmpl" %}
{% block title %}
   Profile View  
{% endblock %}

{% block content %}
     
<div class="colmask threecol">
     <div class="colmid">
	  <div class="colleft">
	       <div class="col1">
                    {% if profile and profile.schedule %}
                    <div class='landing_box'>
			 {% include 'gbe/incl_schedule.tmpl' %}
		    </div>
		    &nbsp;
		    {% endif %}
                    {% if profile and tickets %}
                    <div class='landing_box'>
			 {% include 'gbe/incl_tickets.tmpl' %}
		    </div>
		    {% endif %}
	       </div>
	       <div class="col2">
		    {% include 'gbe/incl_lp_left.tmpl' %}
	       </div>
	       <div class="col3">
                    {% if profile %}
                    <div class='landing_box'>
                        {% include 'gbe/incl_lp_right.tmpl' %}
                    </div>
                    {% endif %}
	       </div>
	  </div>
     </div>
</div>


{% endblock %}
