	{% if classes %}
	   <span class='heading classes'> 
	    {{ profile.display_name }} has proposed these classes
	   </span><br>
	   {% if classes %} 
	   {% for class in classes %}
	     <span class='class'>{{ class }} </span> <br>
	     <a href='/class/edit/{{class.id}}'>Edit this class</a> <br>
	     {% for alert in class.alerts %}
	     <span class='alert'>{{alert | safe }}</a>
	     {% endfor %}	     
	<a href='/class/create'>Propose another class</a>
	  {% endfor %}
	  {%endif %}
	{% else %}
	   {{ profile.display_name }} has not proposed any classes
	<a href='/class/create'>Propose a class</a>
	{% endif %}
	

<br>